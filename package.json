{
  "name": "gametok-multiagent",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@8.15.4",
  "scripts": {
    "build": "pnpm --filter @gametok/schemas build && pnpm --filter @gametok/console build",
    "build:all": "pnpm -r run build",
    "test": "pnpm -r run test",
    "lint": "pnpm -r run lint",
    "dev:console": "pnpm --filter @gametok/console dev",
    "dev:orchestrator": "pnpm --filter services-orchestrator dev",
    "db:migrate": "tsx scripts/run-migrations.ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "tsx": "^4.7.1",
    "typescript": "^5.4.5",
    "vitest": "^1.5.0",
    "postgres": "^3.4.3",
    "eslint-import-resolver-typescript": "^3.5.6"
  }
}
